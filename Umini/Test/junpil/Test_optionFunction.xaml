        <TextBox x:Name="Txtb6" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" Margin="227,122,0,0" TextChanged="TextBox_TextChanged_1"/>
        <Label Content="00:00(00시00분)" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="259,138,0,0"/>
        <RadioButton HorizontalAlignment="Left" VerticalAlignment="Top" Margin="102,31,0,0"/>
