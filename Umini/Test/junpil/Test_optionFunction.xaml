            <ComboBoxItem Content="Alt"/>
            <ComboBoxItem Content="Shift"/>
        </ComboBox>
        <ComboBox x:Name="cb2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="149,214,0,0">
            <ComboBoxItem Content="Ctrl" IsSelected="True"/>
            <ComboBoxItem Content="Alt"/>
            <ComboBoxItem Content="Shift"/>
        </ComboBox>
        <Button x:Name="btnYoutube" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="330,330,0,0" Click="btnYoutube_Click"/>
        <TextBox x:Name="txttesttxt" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="307" Margin="19,373,0,0"/>
